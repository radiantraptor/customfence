{
  "multipart": [
    {
      "when": { "up": "true" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_post" }
    },
    {
      "when": { "north": "low" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_low", "uvlock": "true" }
    },
    {
      "when": { "east": "low" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_low", "y": 90, "uvlock": "true" }
    },
    {
      "when": { "south": "low" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_low", "y": 180, "uvlock": "true" }
    },
    {
      "when": { "west": "low" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_low", "y": 270, "uvlock": "true" }
    },
    {
      "when": { "north": "tall" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_tall", "uvlock": "true" }
    },
    {
      "when": { "east": "tall" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_tall", "y": 90, "uvlock": "true" }
    },
    {
      "when": { "south": "tall" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_tall", "y": 180, "uvlock": "true" }
    },
    {
      "when": { "west": "tall" },
      "apply": { "model": "customfence:block/brick_modern/brick_modern_side_tall", "y": 270, "uvlock": "true" }
    }
  ]
}
